{
    "name": "moonshine/laravel-ui-bridge",
    "description": "Minimum set of dependencies for using MoonShine UI with Laravel",
    "keywords": ["moonshine"],
    "type": "library",
    "homepage": "https://getmoonshine.app",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/moonshine/larave-bridge/issues",
        "source": "https://github.com/moonshine/larave-bridge"
    },
    "authors": [
        {
            "name": "Danil Shutsky",
            "email": "info@cutcode.dev",
            "homepage": "https://cutcode.dev"
        }
    ],
    "require": {
        "php": "^8.2|^8.3|^8.4",
        "ext-curl": "*",
        "ext-json": "*",
        "moonshine/core": "^3.0",
        "moonshine/contracts": "^3.0",
        "moonshine/color-manager": "^3.0",
        "moonshine/menu-manager": "^3.0",
        "illuminate/contracts": "^10|^11|^12",
        "illuminate/support": "^10|^11|^12",
        "illuminate/view": "^10|^11|^12",
        "nyholm/psr7": "^1.8",
        "symfony/http-foundation": "^7.2",
        "symfony/psr-http-message-bridge": "^7.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.68",
        "rector/rector": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "MoonShine\\LaravelUIBridge\\": "src/"
        }
    },
    "conflict": {
        "moonshine/ui": "<3.0"
    },
    "scripts": {
        "rector": "vendor/bin/rector process --clear-cache",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "extra": {
        "laravel": {
            "providers": [
                "MoonShine\\LaravelUIBridge\\LaravelUIBridgeServiceProvider"
            ]
        }
    }
}
